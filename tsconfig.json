{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/components/ui/chat/*": [
        "./registry/components/ui/chat/*"
      ],
      "@/components/ui/input/*": [
        "./registry/components/ui/input/*"
      ],
      "@/components/ui/layout/*": [
        "./registry/components/ui/layout/*"
      ],
      "@/components/ui/media/*": [
        "./registry/components/ui/media/*"
      ],
      "@/components/ui/utils/*": [
        "./registry/components/ui/utils/*"
      ],
      "@/components/hooks/*": [
        "./registry/components/hooks/*"
      ],
      "@/components/providers/*": [
        "./registry/components/providers/*"
      ],
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}